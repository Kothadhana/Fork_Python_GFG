def number_present(num_list, Q):
    #num is a 'list', Q is a 'int'
    for i in num_list:    #write this here - i in range(len(num_list))
                             # see the use of 'in' in for loop
        if (i is Q) : #write this here - num_list[i] is Q
                             # see the use of 'is' as equal to
            return True
    return False 
